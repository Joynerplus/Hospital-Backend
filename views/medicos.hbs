<h1>CENTRO MEDICO BACKEND</h1>
<h2>LISTADO DE MEDICOS </h2>
<table>
<thead>
    <tr>
        <th>Cedula</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Especialidad</th>
        <th>Consultorio</th>
        <th>Correo</th>
        <th>Opciones</th>
    </tr>
</thead>
<tbody>
    {{#each medicos}}
    <tr>
        <td>{{cedula}}</td>
        <td>{{nombre}}</td>
        <td>{{apellido}}</td>
        <td>{{especialidad}}</td>
        <td>{{consultorio}}</td>
        <td>{{correo}}</td>
        <td><a href="/medicos/eliminar/{{cedula}}">Eliminar</a></td>
    </tr>
    {{/each}}
</tbody>
</table>

<div id="botones">
  <input type="button" class="boton" id="botonVolver" value="Volver" onclick="irAlHome()">
  <input type="button" class="boton" id="botonAgregar" value="{{botonAgregar}}" onclick="agregarMedico()">
</div>





