<h1>CENTRO MEDICO HOSPITAL BACKEND</h1>
<h2>{{subtitulo}}</h2>
<table>
<thead>
    <tr>
        <th>Cedula</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Edad</th>
        <th>Telefono</th>  
        <th>Opciones</th>
    </tr>
</thead>
<tbody>
    {{#each medicos}}
    <tr>
        <td>{{cedula}}</td>
        <td>{{nombres}}</td>
        <td>{{apellido}}</td>
        <td>{{edad}}</td>
        <td>{{telefono}}</td>       
        <td><a href="/medicos/eliminar/{{cedula}}">Eliminar</a></td>
    </tr>
    {{/each}}
</tbody>
</table>

<div id="botones">
  <input type="button" class="boton" id="botonVolver" value="Volver" onclick="irAlHome()">
  <input type="button" class="boton" id="botonAgregar" value="{{botonAgregar}}" onclick="agregarPaciente()">
</div>




